                            ;===============================================================================
                            ; Output Utilities
                            ;-------------------------------------------------------------------------------
                            
                            ; Fetch the PC from the stack and display it in hex. Save its value in the
                            ; memory pointer to allow subsequent byte and opcode display.
                            
                            ShowPC:
00E420  FEA006            :                 ldx     SP              ; Handle the PC MSB
00E423  A606              :                 lda a   6,x
00E425  B7A008            :                 sta a   MPTR+0
00E428  8D73              :                 bsr     OutHex2 
00E42A  A607              :                 lda a   7,x             ; .. the the PC LSB
00E42C  B7A009            :                 sta a   MPTR+1
00E42F  206C              :                 bra     OutHex2
                            
                            ; Fetch the status register from the stack and then display its value as
                            ; individual bits.
                            
                            ShowP:
00E431  CEE52D            :                 ldx     #P_STR
00E434  BDE07E            :                 jsr     OutStr
00E437  FEA006            :                 ldx     SP
00E43A  E601              :                 lda b   1,x
00E43C  8631              :                 lda a   #'1'
00E43E  8D1A              :                 bsr     ShowBit
00E440  8631              :                 lda a   #'1'
00E442  8D16              :                 bsr     ShowBit
00E444  8648              :                 lda a   #'H'
00E446  8D12              :                 bsr     ShowBit
00E448  8649              :                 lda a   #'I'
00E44A  8D0E              :                 bsr     ShowBit
00E44C  864E              :                 lda a   #'N'
00E44E  8D0A              :                 bsr     ShowBit
00E450  865A              :                 lda a   #'Z'
00E452  8D06              :                 bsr     ShowBit
00E454  8656              :                 lda a   #'V'
00E456  8D02              :                 bsr     ShowBit
00E458  8643              :                 lda a   #'C'
                            
                            ; Rotate a bit out of the status register in B. If it is clear the change the
                            ; flag character to a full stop.
                            
                            ShowBit:
00E45A  59                :                 rol b
00E45B  2502              :                 bcs     .Skip
00E45D  862E              :                 lda a   #'.'
00E45F  7EE5C9            : .Skip           jmp     UartTx
                            
